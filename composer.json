{
  "name": "bukita/sage-boilerplate",
  "type": "package",
  "description": "A Roots Acorn package for building SSM templates for the SSM ACF starter theme",
  "license": "MIT",
  "authors": [
    {
      "name": "Mariia Shchepeleva",
      "email": "maria@secretstache.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bukita\\SageBoilerplate\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "roots/acorn": "^4.0"
  },
  "extra": {
    "acorn": {
      "providers": [
        "Bukita\\SageBoilerplate\\Providers\\BuildTemplatesServiceProvider"
      ]
    }
  }
}
